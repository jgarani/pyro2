# simple inputs files for the four-corner problem.

[driver]
max_steps = 10000
tmax = 1


[io]
basename = vortex_
n_out = 1


[mesh]
nx = 64
ny = 64
xmax = 1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic


[compressible]
grav = 0.0

limiter = 2
